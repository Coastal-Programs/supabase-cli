{
  "name": "@coastal-programs/supabase-cli",
  "version": "0.1.0",
  "description": "Production-ready Supabase CLI built with oclif + TypeScript",
  "author": "Coastal Programs",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "supabase-cli": "./bin/run"
  },
  "homepage": "https://github.com/coastal-programs/supabase-cli",
  "repository": {
    "type": "git",
    "url": "https://github.com/coastal-programs/supabase-cli.git"
  },
  "bugs": {
    "url": "https://github.com/coastal-programs/supabase-cli/issues"
  },
  "keywords": [
    "supabase",
    "cli",
    "oclif",
    "typescript",
    "api"
  ],
  "engines": {
    "node": ">=22.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc.json",
    "lint:fix": "eslint . --ext .ts --config .eslintrc.json --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "test:coverage": "nyc --reporter=lcov --reporter=text --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "test:performance": "mocha --timeout 120000 \"test/performance/**/*.test.ts\"",
    "test:performance:quick": "mocha --timeout 60000 \"test/performance/benchmarks.test.ts\"",
    "test:watch": "mocha --watch --watch-extensions ts --require ts-node/register \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "readme": "oclif readme",
    "postinstall": "node scripts/postinstall.js"
  },
  "dependencies": {
    "@oclif/core": "^2.15.0",
    "@oclif/plugin-help": "^5.2.20",
    "@oclif/plugin-plugins": "^3.9.0",
    "@oclif/plugin-update": "^3.2.4",
    "@oclif/plugin-warn-if-update-available": "^2.1.1",
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.3",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "inquirer": "^8.2.6",
    "listr2": "^7.0.2",
    "lodash": "^4.17.21",
    "ora": "^5.4.1",
    "quick-lru": "^5.1.1",
    "tslib": "^2.6.2",
    "update-notifier": "^6.0.2"
  },
  "devDependencies": {
    "@oclif/test": "^2.5.6",
    "@types/chai": "^4.3.11",
    "@types/inquirer": "^8.2.10",
    "@types/lodash": "^4.14.202",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.6",
    "@types/sinon": "^17.0.2",
    "@types/update-notifier": "^6.0.8",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "chai": "^4.3.10",
    "eslint": "^8.56.0",
    "eslint-config-oclif": "^5.0.0",
    "eslint-config-oclif-typescript": "^3.0.8",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "oclif": "^3.17.2",
    "prettier": "^3.1.1",
    "shx": "^0.3.4",
    "sinon": "^17.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "oclif": {
    "bin": "supabase-cli",
    "dirname": "supabase-cli",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins",
      "@oclif/plugin-update",
      "@oclif/plugin-warn-if-update-available"
    ],
    "topicSeparator": ":",
    "topics": {
      "projects": {
        "description": "Manage Supabase projects"
      },
      "config": {
        "description": "Configure CLI settings"
      }
    },
    "hooks": {}
  }
}
